        "klassifikation": {
          "tore_bayern": 1,
          "tore_gegner": 0,
          "phase": 2
        }
      },
      {
        "index": 61,
        "text": "Henrichs direkt, Sesko, Benjaminko gegen Manuel Neuer.",
