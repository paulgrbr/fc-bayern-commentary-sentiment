        "klassifikation": {
          "tore_bayern": 1,
          "tore_gegner": 0,
          "phase": 2
        }
      },
      {
        "index": 63,
        "text": "Der 20-jährige Slowene seit Wochen unwiderstehlich vor des Gegners Tor.",
