        "kontext": "Neutral",
        "klassifikation": {
          "tore_bayern": 5,
          "tore_gegner": 1,
          "phase": 2
        }
      },
      {
        "index": 95,
        "text": "Harry Kane mit Tor Nummer 3 an diesem Abend, das 6:1 in der Nachspielzeit.",
