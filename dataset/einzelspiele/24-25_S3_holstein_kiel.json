        "klassifikation": {
          "tore_bayern": 5,
          "tore_gegner": 1,
          "phase": 2
        }
      },
      {
        "index": 84,
        "text": "Noch mal die Münchner.",
        "kontext": "FC Bayern München",
        "klassifikation": {
          "tore_bayern": 5,
          "tore_gegner": 1,
          "phase": 2
        }
      },
      {
        "index": 85,
        "text": "Palhinha, Müller schiebt weiter, Kane, Harry Kane geht zu Boden.",
