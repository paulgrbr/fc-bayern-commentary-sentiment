        "klassifikation": {
          "tore_bayern": 0,
          "tore_gegner": 0,
          "phase": 2
        }
      },
      {
        "index": 63,
        "text": "Die Frage: War da eine Hand im Spiel?",
        "kontext": "Neutral",
        "klassifikation": {
          "tore_bayern": 0,
          "tore_gegner": 0,
          "phase": 2
        }
      },
      {
        "index": 64,
        "text": "Erstmal bei Upamecano, Bayerns bestem, alles sauber.",
