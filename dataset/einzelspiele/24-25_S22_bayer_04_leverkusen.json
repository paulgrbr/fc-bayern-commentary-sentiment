        "klassifikation": {
          "tore_bayern": 0,
          "tore_gegner": 0,
          "phase": 2
        }
      },
      {
        "index": 73,
        "text": "Nächste Großchance Leverkusen, eine Stunde fast rum.",
        "kontext": "Bayer 04 Leverkusen",
