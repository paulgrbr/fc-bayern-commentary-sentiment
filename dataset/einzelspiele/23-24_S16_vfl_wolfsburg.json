        "klassifikation": {
          "tore_bayern": 2,
          "tore_gegner": 1,
          "phase": 2
        }
      },
      {
        "index": 71,
        "text": "Und es ist wieder Maxi Arnold, der einen raushaut.",
        "kontext": "VfL Wolfsburg",
        "klassifikation": {
