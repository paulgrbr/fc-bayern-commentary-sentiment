        "klassifikation": {
          "tore_bayern": 2,
          "tore_gegner": 1,
          "phase": 2
        }
      },
      {
        "index": 68,
        "text": "Aber ein starker Angriff der Münchner aus der eigenen Defensive.",
        "kontext": "FC Bayern München",
        "klassifikation": {
          "tore_bayern": 2,
          "tore_gegner": 1,
          "phase": 2
        }
      },
      {
        "index": 69,
        "text": "Was kommt noch von den Wolfsburgern?",
        "kontext": "VfL Wolfsburg",
        "klassifikation": {
          "tore_bayern": 2,
          "tore_gegner": 1,
          "phase": 2
        }
      },
      {
        "index": 70,
