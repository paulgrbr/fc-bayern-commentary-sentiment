        "klassifikation": {
          "tore_bayern": 1,
          "tore_gegner": 0,
          "phase": 2
        }
      },
      {
        "index": 51,
        "text": "Eben die 18. Vorlage: Michael Olise jetzt gegen Ulrich.",
